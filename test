#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int main()
{ 
	int i = 1;
	while (i <= 10)
	{
		if (i == 5)
			continue;
		printf("i= %d\n", i);
		i++;
	}
	return 0;
}
